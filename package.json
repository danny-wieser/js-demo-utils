{
  "name": "redux-service-demo",
  "version": "1.0.4",
  "description": "A simple React UI for testing Redux-based data services",
  "main": "dist/index.js",
  "repository": "git@github.com:danny-wieser/js-demo-utils.git",
  "author": "<danny.wieser@gmail.com>",
  "license": "MIT",
  "homepage": "https://danny-wieser.github.io/redux-service-demo/",
  "scripts": {
    "clean": "rimraf dist coverage yarn-error.log demo/dist",
    "build": "yarn run clean && babel src --out-dir dist --ignore '**/*.spec.js' && yarn run css-build",
    "docs": "jsdoc src/*.js",
    "test": "jest --coverage --collectCoverageFrom=src/** && yarn run lint",
    "demo": "webpack-serve --config demo/webpack.config.js --content demo",
    "lint": "eslint .",
    "css-build": "node-sass --omit-source-map-url src/index.scss styles/index.css",
    "predeploy": "yarn run clean && webpack --config demo/webpack.config.js --mode=production",
    "deploy": "gh-pages -d demo"
  },
  "files": [
    "dist/**/*.*",
    "styles/**/*.*"
  ],
  "jest": {
    "setupTestFrameworkScriptFile": "./setupTests.js"
  },
  "dependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "bulma": "^0.7.1",
    "css-loader": "^1.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.18.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "gh-pages": "^1.2.0",
    "jest": "^23.4.1",
    "node-sass": "^4.9.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "webpack": "^4.16.2",
    "webpack-cli": "^3.1.0",
    "webpack-serve": "^2.0.2",
    "whatwg-fetch": "^2.0.4"
  }
}
