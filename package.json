{
  "name": "redux-service-demo",
  "version": "1.1.11",
  "description": "A simple React UI for testing Redux-based data services",
  "main": "dist/index.js",
  "repository": "git@github.com:danny-wieser/js-demo-utils.git",
  "author": "<danny.wieser@gmail.com>",
  "license": "MIT",
  "homepage": "https://danny-wieser.github.io/redux-service-demo/",
  "scripts": {
    "clean": "rimraf dist coverage yarn-error.log demo/dist",
    "build": "babel src --out-dir dist --ignore '**/*.spec.js' && yarn run css-build",
    "start": "yarn run clean && babel src --out-dir dist --ignore '**/*.spec.js' --watch",
    "test": "jest --coverage --collectCoverageFrom=src/** && yarn run lint",
    "demo": "webpack-serve --config demo/webpack.config.js --content demo",
    "lint": "eslint .",
    "css-build": "node-sass --omit-source-map-url src/index.scss styles/index.css",
    "demobuild": "webpack --config demo/webpack.config.js --mode=production",
    "versionbump": "npm version patch -m \"Bumping to %s\" && git push",
    "deploy": "yarn clean && yarn versionbump && yarn build && yarn add-version && yarn demobuild",
    "add-version": "replace -s \"@VERSION@\" $npm_package_version dist/*.js"
  },
  "files": [
    "dist/**/*.*",
    "styles/**/*.*"
  ],
  "jest": {
    "setupTestFrameworkScriptFile": "./setupTests.js"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bulma": "^0.7.1",
    "loglevel": "^1.6.1",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-service-util": "1.0.7",
    "redux-thunk": "^2.3.0",
    "url-search-params-polyfill": "^4.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.0",
    "css-loader": "^1.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.18.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "gh-pages": "^1.2.0",
    "jest": "^23.5.0",
    "node-sass": "^4.9.2",
    "regenerator-runtime": "^0.12.1",
    "replace": "^1.0.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "webpack": "^4.16.2",
    "webpack-cli": "^3.1.0",
    "webpack-serve": "^2.0.2",
    "whatwg-fetch": "^2.0.4"
  }
}
